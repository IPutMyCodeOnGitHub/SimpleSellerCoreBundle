    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "3.4.*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        }
    }
}

{
    "name": "simpleseller/core-bundle",
    "description": "A core bundle of imagined e-commerce platform",
    "type": "symfony-bundle",
    "authors": [{
        "name": "Nikolay Nikitin",
        "email": "nikolay.nikitin@noveogroup.com"
    }],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SimpleSeller\\CoreBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
